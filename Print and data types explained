print ("hello")
print ("rafi")
print ("amin")#This is a string 
print (4)#This is an integer
print (4.4)#This is a real/float 
print ("^")#This is a character 
print (True)#This is a boolean

#Data types
#CRIBS
#C = CHARACTER E.G."A"
#R = REAL/FLOAT E.G. 4.66
#I = INTEGER E.G. 222
#B = BOOLEAN E.G. TRUE
#S = STRING E.G. "HELLO"
